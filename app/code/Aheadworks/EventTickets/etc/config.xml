<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <aw_event_tickets>
            <general>
                <create_ticket_by_order_status>complete</create_ticket_by_order_status>
                <ticket_require_shipping>0</ticket_require_shipping>
                <hide_past_events>0</hide_past_events>
            </general>
            <email>
                <sender>general</sender>
                <new_ticket_template>aw_event_tickets_email_new_ticket_template</new_ticket_template>
                <ticket_template_pdf>aw_event_tickets_email_ticket_template_pdf</ticket_template_pdf>
            </email>
            <number_pattern>
                <number_length>12</number_length>
                <number_format>alphanumeric</number_format>
            </number_pattern>
        </aw_event_tickets>
    </default>
</config>
